{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "-webkit-tap-highlight-color",
          "overscroll-behavior"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 118",
    "not firefox <= 117",
    "not safari <= 16.6"
  ]
}